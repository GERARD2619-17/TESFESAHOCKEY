{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "axe/forms": "off",
    "axe/name-role-value": "off",
    "apple-touch-icons": "off",
    "compat-api/css": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "meta-viewport": "off",
    "axe/aria": [
      "default",
      {
        "empty-table-header": "off"
      }
    ],
    "highest-available-document-mode": "off"
  }
}